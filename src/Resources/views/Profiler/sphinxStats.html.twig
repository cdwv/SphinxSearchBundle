{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    <div class="sf-toolbar-block">
        <div class="sf-toolbar-info">
            <div class="sf-toolbar-info-piece">
                <b>Queries: </b>
                <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.data.queries|length }}</span>
            </div>
            <div class="sf-toolbar-info-piece">
                <b>Execution time: </b>
                <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.data.time_summary|number_format(2) }} ms</span>
            </div>
            {% if collector.data.errors|length != 0 %}
                <div class="sf-toolbar-info-piece">
                    <b>Errors: </b>
                    <span class="sf-toolbar-status sf-toolbar-status-red">{{ collector.data.errors|length }}</span>
                </div>
                <div class="sf-toolbar-info-piece">
                    <b>Last error: </b>
                    {{ (collector.data.errors|last).message }}
                </div>
            {% endif %}
        </div>
        <div class="sf-toolbar-icon">
            <a href="{{ path('_profiler', { 'token': token, 'panel': name }) }}">
            <span class="icon"> <img src="data:image/png;base64,

" alt="sphinx" /> </span>
                {#<span class="icon"><img src="data:image/png;base64,..." alt="" /></span>#}
                {% set error_class = collector.data.errors|length == 0 ? "" : "sf-toolbar-status-red" %}
                <span class="sf-toolbar-status {{ error_class }}">{{ collector.data.queries|length }}</span>
            </a>
        </div>
    </div>
{% endblock %}


{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64,

" alt="" /></span>
        <strong>Sphinx</strong>
        <span class="count">
            <span>{{ collector.data.time_summary|number_format(2) }} ms</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>SphinxSearchBundle</h2>

    {% if collector.data.errors|length == 0 %}
        <p><em>No errors were triggered.</em></p>
    {% else %}
        <table>
            <tr>
                <th>#</th>
                <th>Error</th>
                <th>#query</th>
                <th>query</th>
            </tr>

            {% for error in collector.data.errors %}
                <tr>
                    <td>{{ loop.index }}</td>
                    <td>{{ error.message }}</td>
                    {% if error.query is not null %}
                        <td> {{ error.query + 1 }}</td>
                        <td> {{ collector.data.queries[error.query].toString }}</td>
                    {% else %}
                        <td></td>
                        <td></td>
                    {% endif %}
                </tr>
            {% endfor %}
        </table>
    {% endif %}

    {% if collector.data.queries|length == 0 %}
        <p><em>No queries were executed.</em></p>
    {% else %}
        <table>
            <tr>
                <th>#</th>
                <th>Query</th>
                <th>Duration</th>
                <th>Success</th>
            </tr>

            {% for query in collector.data.queries %}
                <tr>
                    <td>{{ loop.index }}</td>
                    <td>{{ query.toString }}</td>
                    <td>{{ query.time|number_format(2) }} ms</td>
                    <td>{% if query.success %} OK {% else %} nope {% endif %}</td>
                </tr>
            {% endfor %}
        </table>
    {% endif %}
{% endblock %}